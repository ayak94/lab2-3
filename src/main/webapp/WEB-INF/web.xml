<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="3.1">
    <servlet>
        <servlet-name>ServletLab3</servlet-name>
        <servlet-class>com.yakimovaann.labs.ServletLab3</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletLab3</servlet-name>
        <url-pattern>/servletLab3</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ServletLab2</servlet-name>
        <servlet-class>com.yakimovaann.labs.ServletLab2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServletLab2</servlet-name>
        <url-pattern>/servletLab2</url-pattern>
    </servlet-mapping>

    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>